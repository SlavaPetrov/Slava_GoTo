    "    t = []\n",
    "    k = s.split(', ')\n",
    "    for i in k:\n",
    "        k1 = i.split(' ')\n",
    "        t += [k1[0]] * int(k1[1])\n",
    "    t.sort()\n",
    "    print(', '.join(t)) \n",
    "sor (input())    "
